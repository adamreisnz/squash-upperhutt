{
  "name": "squash-upperhutt",
  "description": "Website for Squash @ Upper Hutt",
  "version": "1.0.0",
  "homepage": "https://www.squashupperhutt.co.nz/",
  "author": {
    "name": "Adam Reis",
    "email": "adam@reis.nz",
    "url": "http://adam.reis.nz/"
  },
  "contributors": [
    {
      "name": "Liat Reis",
      "email": "liat@reis.nz",
      "url": "http://liat.reis.nz/"
    }
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/adamreisnz/squash-upperhutt"
  },
  "bugs": {
    "url": "https://github.com/adamreisnz/squash-upperhutt/issues"
  },
  "keywords": [],
  "engines": {
    "node": "^7.1.0",
    "npm": "^4.0.2"
  },
  "scripts": {
    "reinstall": "rimraf node_modules && npm cache clean && npm install",
    "build:dev": "gulp build --env dev",
    "build:prod": "gulp build --env production",
    "preserve": "lsof -n -i:8080 | grep LISTEN | awk '{print $2}' | xargs kill -9",
    "serve": "node build/serve.js --env dev",
    "prestart": "npm run build:dev -s",
    "start": "parallelshell \"npm run serve -s\" \"npm run watch -s\"",
    "lint": "eslint . --fix",
    "watch": "gulp watch",
    "postversion": "git commit -a -m \"[bump] $npm_package_version\"",
    "predeploy": "npm run build:prod",
    "deploy": "s3-cli sync --region ap-southeast-2 --delete-removed ./dist/production s3://www.squashupperhutt.co.nz/",
    "sentry:release": "sentry-cli releases -o reis -p bookingpro new $npm_package_version upload-sourcemaps ./dist/staging/bundles"
  },
  "dependencies": {
    "vue": "^2.1.4"
  },
  "devDependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.17.5",
    "chalk": "^1.1.3",
    "del": "^2.2.2",
    "eslint": "^3.9.0",
    "gulp": "git+https://github.com/gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-csso": "^2.0.0",
    "gulp-file": "^0.3.0",
    "gulp-htmlclean": "^2.7.6",
    "gulp-inject": "^4.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-preprocess": "^2.0.0",
    "gulp-remove-empty-lines": "^0.0.8",
    "gulp-remove-html-comments": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "http-proxy-middleware": "^0.17.2",
    "mime": "^1.3.4",
    "parallelshell": "^2.0.0",
    "phantomjs-prebuilt": "^2.1.13",
    "rimraf": "^2.5.4",
    "s3-cli": "^0.13.0",
    "serve-static": "^1.11.1",
    "yamljs": "^0.2.8",
    "yargs": "^6.3.0"
  }
}
