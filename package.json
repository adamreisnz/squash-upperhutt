  "bugs": {
    "url": "https://github.com/adamreisnz/squash-upperhutt/issues"
  },
  "keywords": [],
  "engines": {
    "node": "^7.4.0"
  },
  "scripts": {
    "reinstall": "rimraf node_modules && yarn cache clean && yarn install",
    "build": "gulp build",
    "build:dev": "gulp build --env dev",
    "build:prod": "gulp build --env production",
    "serve": "node build/serve.js --env dev",
    "preserve": "lsof -n -i:8080 | grep LISTEN | awk '{print $2}' | xargs kill -9",
    "prestart": "yarn run build -s",
    "start": "parallelshell \"yarn run serve -s\" \"yarn run watch -s\"",
    "dev": "yarn start",
    "lint": "eslint . --fix",
    "pretest": "yarn run lint -s",
    "watch": "gulp watch",
    "postversion": "git commit -a -m \"[bump] $npm_package_version\" && git push"
  },
  "dependencies": {
    "angular": "1.6.1",
    "angular-ui-router": "1.0.0-rc.1",
    "meanie-angular-log": "^1.2.1"
  },
  "devDependencies": {
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.5",
    "chalk": "^1.1.3",
    "del": "^2.2.2",
    "git-rev-sync": "^1.8.0",
    "gulp": "git+https://github.com/gulpjs/gulp#4.0",
    "gulp-angular-templatecache": "^2.0.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^2.0.0",
    "gulp-file": "^0.3.0",
    "gulp-htmlclean": "^2.7.8",
    "gulp-inject": "^4.2.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-ng-constant": "^1.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-preprocess": "^2.0.0",
    "gulp-remove-empty-lines": "^0.0.8",
    "gulp-remove-html-comments": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.0.0",
    "gulp-sourcemaps": "^2.2.1",
    "gulp-uglify": "^2.0.0",
    "gulp-wrapper": "^1.0.0",
    "merge-stream": "^1.0.1",
    "parallelshell": "^2.0.0",
    "replace-in-file": "^2.0.3",
    "rimraf": "^2.5.4",
    "serve-static": "^1.11.1",
    "yargs": "^6.5.0"
  }
}
